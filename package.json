{
  "name": "js-scratch",
  "version": "1.0.0",
  "description": "",
  "main": "eslint.config.js",
  "type": "module",
  "workspaces": [
    "packages/*",
    "applications/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "clean": "npm run clean:node_modules && npm run clean --workspaces --if-present",
    "clean:node_modules": "rm -rf node_modules",
    "dev": "npm run dev --workspaces --if-present",
    "format": "prettier . -c",
    "format:fix": "prettier . -w",
    "install:root": "npm install --workspaces=false",
    "lint": "eslint . --fix",
    "reset": "npm run clean && npm install && npm run build",
    "test": "npm run test --workspaces --if-present",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.14.11",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "3.3.3",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
